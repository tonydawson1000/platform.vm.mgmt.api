{
  "ConnectionStrings": {
    "PlatformVmMgmtConnectionString": "<TOP-SECRET>"
  },
  "EmailSettings": {
    "ApiKey": "<TOP-SECRET>",
    "FromAddress": "noreply@platformvmmgmt.me",
    "FromName": "Platform Bot"
  },
  "SlackSettings": {
    "ApiKey": "<TOP-SECRET>"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Filter": [
      {
        "Name": "ByExcluding",
        "Args": { "expression": "@mt = 'An unhandled exception has occurred while executing the request.'" }
      }
    ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}